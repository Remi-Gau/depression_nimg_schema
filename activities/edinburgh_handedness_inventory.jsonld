{
    "@context": "https://raw.githubusercontent.com/ReproNim/reproschema/1.0.0-rc1/contexts/generic",
    "@type": "reproschema:Activity",
    "@id": "edinburgh_handedness_inventory.jsonld",
    "prefLabel": "Edinburgh handedness inventory",
    "description": "Edinburgh handedness inventory",
    "schemaVersion": "1.0.0-rc1",
    "version": "0.0.1",
    "citation": [
        "10.1016/0028-3932(71)90067-4",
        "10.1080/1357650X.2013.783045"
        ],
    "preamble": "Please indicate your preferences in the use of hands in the following activities or objects:",
    "ui": {
        "order": [
            "items/writing.jsonld",
            "items/drawing.jsonld",
            "items/EHI_results.jsonld"
        ],
        "shuffle": false,
        "addProperties": [
            {
                "variableName": "writing",
                "isAbout": "items/writing.jsonld",
                "valueRequired": true,
                "isVis": true
            },
            {
                "variableName": "drawing",
                "isAbout": "items/drawing.jsonld",
                "valueRequired": true,
                "isVis": true
            },
            {
                "isAbout": "items/EHI_results.jsonld",
                "variableName": "EHI_results",
                "isVis": true
            }
        ]
    },
    "compute": [
        {
            "variableName": "EHI_results",
            "jsExpression": "( writing + drawing ) / 2"
        }
    ]
}